{
  "name": "A fuckin' chrome extension",
  "version": "1.0",
  "description": "This is chrome extension!",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"

  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["script.js"],
    "persistent": true
  },

  "content_scripts":[{
    "matches": ["http://*/*","https://*/*" ],
    "js": ["jquery-3.3.1.min.js", "script.js"]
  }]
}
